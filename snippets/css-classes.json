{
	// Display utilities
	"Utility: Display": {
		"prefix": ".d-value",
		"body": [
			".d-${1|flex,inline-flex,none,inline,inline-block,block,table,table-cell,table-row|}"
		],
		"description": ".d-{value}"
	},
	"Utility: Display (Breakpoint)": {
		"prefix": ".d-breakpoint-value",
		"body": [
			".d-${1:breakpoint}-${2|flex,inline-flex,none,inline,inline-block,block,table,table-cell,table-row|}"
		],
		"description": ".d-{breakpoint}-{value}"
	}
}